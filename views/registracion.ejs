<body>
    <header class="uk-background-primary">
        <section class=" uk-container uk-container-large">
            <div class="">
                <nav class="" uk-navbar>
                    <div class="uk-navbar-left">
                        <h2 class="logo"><a href="home.html">MoINCZ</a></h2>
                    </div>
                    <div class="uk-navbar-center">
                        <ul class="uk-flex uk-flex-between uk-flex-middle">
                            <li><a href="/">Home</a></li>
                            <li><a href="/post/agregar">Postear</a></li>
                            <li><a href="/registracion">Registración</a></li>
                            <li><a href="/login">Login</a></li>
                        </ul>
                    </div>
                    <div class="uk-navbar-right">
                        <ul class="uk-flex uk-flex-between uk-flex-middle">
                            <li>
                                <form action="" class="uk-search uk-search-default">
                                    <span uk-search-icon></span>
                                    <input type="search" class="uk-search-input" placeholder="Buscar...">
                                </form>
                            </li>
                        </ul>
                    </div>
                </nav>
            </div>
        </section>
    </header>

    <main>
        <div class="uk-section uk-section-small">
            <div class="uk-container">
                <form action="/">
                    <fieldset class="uk-fieldset">
                
                        <legend class="uk-legend">Registracion</legend>
                
                        <div class="uk-margin">
                            <input class="uk-input" type="text" placeholder="nombre">
                        </div>
                        <div class="uk-margin">
                            <input class="uk-input" type="text" placeholder="apellido">
                        </div>
                        <div class="uk-margin">
                            <input class="uk-input" type="text" placeholder="username">
                        </div>
                        <div class="uk-margin">
                            <input class="uk-input" type="email" placeholder="ejemplo@mail.com">
                        </div>
                        <div class="uk-margin">
                            <input class="uk-input" type="password" placeholder="contraseña">
                        </div>
                        <div class="uk-margin">
                            <h5 class="uk-form-label">Fecha de nacimiento</h5>
                            <input class="uk-input" type="date">
                        </div>
                        <div class="uk-margin">
                            <input type="submit" class="uk-button uk-button-primary uk-width-1-1 uk-margin-small-bottom" value="Quiero ser parte de MoINCZ!"></input>
                        </div>

                
                    </fieldset>
                </form>
            </div>
        </div>
